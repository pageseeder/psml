                    <mn>1</mn>
                    <mo>+</mo>
                    <mfrac>
                      <mn>2</mn>
                      <mrow>
                        <mn>1</mn>
                        <mo>+</mo>
                        <mfrac>
                          <mn>2</mn>
                          <mn>1</mn>
                        </mfrac>
                      </mrow>
                    </mfrac>
                  </mrow>
                </mfrac>
              </mrow>
            </math>
          </media-fragment>
        </xref>
        .
      </para>
    </fragment>
